<Window x:Class="TestNET60.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestNET60"
        Title="{Binding Title}"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Height="450" Width="800">
    <Grid>
        <Button Content="{Binding Title}" Command="{Binding TestBtnCommand}" HorizontalAlignment="Center" VerticalAlignment="Center" Height="55" Width="135"/>

    </Grid>
</Window>
